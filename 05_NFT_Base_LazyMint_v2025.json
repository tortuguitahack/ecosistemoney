      "typeVersion": 1,
      "position": [240, 300]
    },
    {
      "parameters": {
        "jsCode": "// Generate high-value NFT collection concepts for Base blockchain\nconst collections = [\n  {\n    name: \"AI Art Genesis\",\n    theme: \"AI-generated abstract art with unique algorithms\",\n    total_supply: 100,\n    mint_price: "0.05", // ETH on Base\n    royalty_percentage: 7.5,\n    target_market: "tech_art_collectors",\n    estimated_floor_price: "0.15", // Expected resale value\n    collection_revenue: 5.0 // 100 * 0.05\n  },\n  {\n    name: \"Metaverse Real Estate\",\n    theme: \"Virtual land parcels in premium metaverse locations\",\n    total_supply: 50,\n    mint_price: "0.08", // ETH on Base\n    royalty_percentage: 10,\n    target_market: "metaverse_investors",\n    estimated_floor_price: "0.25", // Expected resale value\n    collection_revenue: 4.0 // 50 * 0.08\n  },\n  {\n    name: \"Gaming Legends\",\n    theme: \"Rare in-game items and character skins\",\n    total_supply: 200,\n    mint_price: "0.03", // ETH on Base\n    royalty_percentage: 5,\n    target_market: "gaming_community",\n    estimated_floor_price: "0.12", // Expected resale value\n    collection_revenue: 6.0 // 200 * 0.03\n  }\n];\n\n// Select collection based on market analysis\nconst selectedCollection = collections[Math.floor(Math.random() * collections.length)];\nconst marketDemand = 0.78 + (Math.random() * 0.20); // 78-98% demand\nconst estimatedRevenue = selectedCollection.collection_revenue * marketDemand;\n\nreturn [{\n  json: {\n    collection: selectedCollection,\n    market_demand: marketDemand,\n    estimated_revenue: estimatedRevenue,\n    gas_optimization: {\n      chain: "base",\n      estimated_gas_cost: "0.002", // ETH\n      gas_efficiency_score: 92\n    },\n    timestamp: new Date().toISOString()\n  }\n}];"
      },
      "id": "b2c3d4e5-6f7a-8b9c-0d1e-2f3a4b5c6d7e",
      "name": "Generate Collection Concept",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [460, 300]
    },
    {
      "parameters": {
        "url": "https://api.openai.com/v1/chat/completions",
        "authentication": "predefinedCredentialType",
        "nodeCredentialType": "openAiApi",
        "sendHeaders": true,
        "headerParameters": {
          "parameters": [
            {
              "name": "Content-Type",
              "value": "application/json"
            }
          ]
        },
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "model",
              "value": "gpt-4"
            },
            {
              "name": "messages",
              "value": "[{ \"role\": \"user\", \"content\": `Create ${$json.collection.total_supply} unique NFT metadata objects for a ${$json.collection.name} collection with theme: ${$json.collection.theme}. Each NFT should have: title, description, attributes (rarity, power level, etc.), and image_prompt. Return as JSON array.` }]"
            },
            {
              "name": "temperature",
              "value": "0.9"
            },
            {
              "name": "max_tokens",
              "value": "8000"
            }
          ]
        }
      },
      "id": "c3d4e5f6-7a8b-9c0d-1e2f-3a4b5c6d7e8f",
      "name": "AI NFT Metadata Generator",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4,
      "position": [680, 300],
      "credentials": {
        "openAiApi": {
          "id": "openai_credentials",
          "name": "OpenAI API"
        }
      }
    },
    {
      "parameters": {
        "jsCode": "// Process AI-generated metadata and create IPFS-ready data\nconst metadata = JSON.parse($input.first().json.choices[0].message.content);\nconst collectionData = $('Generate Collection Concept').first().json.collection;\n\nconst nftTokens = metadata.map((item, index) => {\n  // Add rarity distribution\n  const rarity = Math.random();\n  let rarityLevel = 'common';\n  if (rarity > 0.9) rarityLevel = 'legendary';\n  else if (rarity > 0.7) rarityLevel = 'epic';\n  else if (rarity > 0.5) rarityLevel = 'rare';\n  else if (rarity > 0.3) rarityLevel = 'uncommon';\n\n  return {\n    token_id: index + 1,\n    name: item.title,\n    description: item.description,\n    attributes: [\n      ...item.attributes,\n      { trait_type: 'rarity', value: rarityLevel },\n      { trait_type: 'collection', value: collectionData.name },\n      { trait_type: 'chain', value: 'base' }\n    ],\n    image_prompt: item.image_prompt,\n    royalty_percentage: collectionData.royalty_percentage,\n    estimated_resale_value: collectionData.estimated_floor_price\n  };\n});\n\n// Calculate collection statistics\nconst rarityDistribution = nftTokens.reduce((acc, token) => {\n  const rarity = token.attributes.find(a => a.trait_type === 'rarity')?.value || 'common';\n  acc[rarity] = (acc[rarity] || 0) + 1;\n  return acc;\n}, {});\n\nreturn [{\n  json: {\n    nft_tokens: nftTokens,\n    collection_stats: {\n      total_supply: nftTokens.length,\n      rarity_distribution: rarityDistribution,\n      avg_resale_value: collectionData.estimated_floor_price,\n      royalty_revenue_potential: nftTokens.length * parseFloat(collectionData.estimated_floor_price) * 0.075\n    },\n    ipfs_config: {\n      gateway: "https://ipfs.io/ipfs/",\n      pinning_service: "pinata"\n    },\n    timestamp: new Date().toISOString()\n  }\n}];"
      },
      "id": "d4e5f6a7-8b9c-0d1e-2f3a-4b5c6d7e8f9a",
      "name": "Process NFT Metadata",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [900, 300]
    },
