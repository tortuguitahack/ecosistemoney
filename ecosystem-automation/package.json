    "drizzle-orm": "^0.30.0",
    "mysql2": "^3.6.5",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.16.0",
    "stripe": "^14.10.0",
    "zod": "^3.22.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.3",
    "lucide-react": "^0.303.0",
    "clsx": "^2.1.0",
