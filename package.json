{
  "name": "automation-ecosystem",
  "version": "2025.1.0",
  "description": "Ecosystem of Automation for Digital Monetization - Production-ready platform with 8 high-revenue workflows",
  "author": "MiniMax Agent",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "automation",
    "n8n",
    "workflows",
    "revenue",
    "affiliate",
    "nft",
    "defi",
    "ai",
    "typescript",
    "react",
    "trpc"
  ],
  "homepage": "https://github.com/yourusername/automation-ecosystem#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/automation-ecosystem.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "main": "server/index.ts",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "vite",
    "dev:db": "docker-compose up -d mysql redis",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "tsc",
    "start": "node dist/server/index.js",
    "start:prod": "NODE_ENV=production npm run start",
    
    "db:setup": "npm run db:create && npm run db:migrate && npm run db:seed",
    "db:create": "tsx scripts/create-database.ts",
    "db:migrate": "drizzle-kit migrate:mysql",
    "db:seed": "node scripts/seed-database.mjs",
    "db:reset": "npm run db:drop && npm run db:setup",
    "db:drop": "tsx scripts/drop-database.ts",
    "db:test": "tsx scripts/test-connection.ts",
    "db:backup": "bash scripts/backup.sh",
    "db:restore": "tsx scripts/restore-database.ts",
    
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,html}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css,html}\"",
    "type-check": "tsc --noEmit",
    "types:generate": "drizzle-kit generate:mysql",
    
    "test": "vitest",
    "test:unit": "vitest run --reporter=verbose",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    
    "docker:build": "docker build -t automation-ecosystem .",
    "docker:run": "docker run -p 3000:3000 automation-ecosystem",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    
    "deploy:vercel": "vercel --prod",
    "deploy:railway": "railway up",
    "deploy:docker": "docker push automation-ecosystem:latest",
    
    "workflows:import": "tsx scripts/import-workflows.ts",
    "workflows:test": "tsx scripts/test-workflows.ts",
    
    "metrics:collect": "curl -s http://localhost:3000/metrics > metrics.prom",
    "health:check": "curl -s http://localhost:3000/health | jq",
    
    "seed:tracking-ids": "node scripts/seed-tracking-ids-simple.mjs",
    "seed:users": "node scripts/seed-users.mjs",
    "seed:full": "npm run db:seed && npm run seed:tracking-ids && npm run seed:users"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-button": "^1.0.4",
    "@radix-ui/react-card": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-form": "^0.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-table": "^1.0.4",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^5.17.0",
    "@tanstack/react-query-devtools": "^5.17.0",
    "@trpc/client": "^11.0.0-rc.320",
    "@trpc/react-query": "^11.0.0-rc.320",
    "@trpc/server": "^11.0.0-rc.320",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.27.2",
    "express": "^4.18.2",
    "lucide-react": "^0.303.0",
    "mysql2": "^3.6.5",
    "node-cron": "^3.0.3",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "recharts": "^2.8.0",
    "stripe": "^14.12.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss": "^4.0.0",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-safe-area": "^0.2.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "drizzle-kit": "^0.20.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "optionalDependencies": {
    "@aws-sdk/client-s3": "^3.485.0",
    "bull": "^4.12.2",
    "redis": "^4.6.12"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,html}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}